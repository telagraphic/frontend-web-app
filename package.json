{
  "dependencies": {
    "gsap": "^3.13.0",
    "nunjucks": "^3.2.4",
    "vite": "^7.1.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build",
    "sass:watch": "sass --watch styles/styles.scss:dist/css/styles.css --style=expanded --source-map",
    "sass:build": "sass styles/styles.scss styles/styles.css --style=compressed --no-source-map",
    "sass:dev": "sass styles/styles.scss:dist/css/styles.css --style=expanded --source-map",
    "sass:check": "sass styles/styles.scss --style=expanded --no-source-map --quiet",
    "build:css": "npm run sass:build",
    "watch:css": "npm run sass:watch",
    "dev:sass": "concurrently \"npm run dev\" \"npm run sass:watch\"",
    "start": "npm run dev",
    "build:prod": "npm run build",
    "clean": "rm -rf dist",
    "clean:build": "npm run clean && npm run build",
    "netlify:dev": "netlify dev --dir=dist",
    "build:pages": "bun run scripts/generate-static-pages.js",
    "build:pages:dev": "NODE_ENV=development bun run scripts/generate-static-pages.js && npm run format:html && mv pages/index.html ./index.html",
    "build:full": "npm run build:css && npm run build",
    "format:html": "prettier --write pages/*.html",
    "format:js": "prettier --write app/**/*.js"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "cssnano": "^7.1.0",
    "glob": "^11.0.3",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.90.0"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "maintained node versions"
  ]
}
