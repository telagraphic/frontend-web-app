{
  "dependencies": {
    "event-emitter": "^0.3.5",
    "gsap": "^3.13.0",
    "prefix": "^1.0.0",
    "vite": "^7.1.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build",
    "sass:watch": "sass --watch styles/styles.scss:dist/css/styles.css --style=expanded --source-map",
    "sass:build": "sass styles/styles.scss styles/styles.css --style=compressed --no-source-map",
    "sass:dev": "sass styles/styles.scss styles/styles.css --style=expanded --source-map",
    "sass:check": "sass styles/styles.scss --style=expanded --no-source-map --quiet",
    "build:css": "npm run sass:build",
    "watch:css": "npm run sass:watch",
    "dev:sass": "concurrently \"npm run dev\" \"npm run sass:watch\"",
    "start": "npm run dev",
    "build:prod": "npm run build",
    "clean": "rm -rf dist",
    "clean:build": "npm run clean && npm run build",
    "netlify:dev": "netlify dev --dir=dist"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "glob": "^11.0.3",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "@types/bun": "latest"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "maintained node versions"
  ],
  "name": "frontend-web-app",
  "private": true,
  "peerDependencies": {
    "typescript": "^5"
  }
}
