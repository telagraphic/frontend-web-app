{
  "dependencies": {
    "gsap": "^3.13.0",
    "nunjucks": "^3.2.4"
  },
  "scripts": {
    "dev": "NODE_ENV=development bun --hot server.js",
    "start": "NODE_ENV=production bun server.js",
    "build": "bun run clean && bun run sass:build & bun run build:js",
    "preview": "bun run build && bun server.js --env production",
    "preview:dist": "bun run scripts/preview-server.js",
    "clean": "rm -rf dist",
    "build:sass": "bun run sass:build --outdir=dist",
    "build:js": "bun build app/App.js --outdir=dist",
    "build:html": "bun run scripts/build-static-pages.js",
    "build:html-dev": "NODE_ENV=development bun run scripts/build-static-pages.js",
    "build:html-prod": "NODE_ENV=production bun run scripts/build-static-pages.js",
    "sass:watch-dev": "sass --watch styles:dist --style=expanded --no-source-map",
    "sass:build": "sass styles/styles.scss dist/styles.css --style=compressed --no-source-map",
    "sass:build-dev": "sass styles/styles.scss dist/styles.css --style=compressed --no-source-map",
    "sass:dev": "sass styles/styles.scss styles/styles.css --style=expanded --source-map",
    "sass:check": "sass styles/styles.scss --style=expanded --no-source-map --quiet",
    "build:css": "npm run sass:build",
    "watch:css": "npm run sass:watch",
    "dev:sass": "concurrently \"npm run dev\" \"npm run sass:watch\"",
    "netlify:dev": "netlify dev --dir=dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "glob": "^11.0.3",
    "nunjucks-cli": "^1.0.0",
    "postcss": "^8.5.6",
    "sass": "^1.90.0"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "maintained node versions"
  ],
  "name": "frontend-web-app",
  "private": true,
  "peerDependencies": {
    "typescript": "^5"
  }
}
